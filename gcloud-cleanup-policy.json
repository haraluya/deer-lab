{
  "rules": [
    {
      "name": "keep-recent-versions",
      "action": {
        "type": "Keep"
      },
      "mostRecentVersions": {
        "keepCount": 3
      }
    },
    {
      "name": "delete-old-versions", 
      "action": {
        "type": "Delete"
      },
      "olderThan": "7d"
    }
  ]
}